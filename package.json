{
    "name": "mdb-theme-fse",
    "description": "Development Scripts",
    "config" : {
        "SASS_SRC": "./assets/src/css",
        "SASS_BLD": "./assets/build/css"
    },
    "scripts": {
        "sass-build-dev": "sass --watch --update --style=expanded assets/src/css/:assets/build/css",
        "sass-build": "sass --watch --update --style=compressed --no-source-map assets/src/css/:assets/build/css/"
    },
    "devDependecies": {
        "sass": "^1.59.3"
    }
}
