{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "color": {
            "palette": [
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#FFFFFF"
                },
                {
                    "name": "Darkness",
                    "slug": "darkness",
                    "color": "hsl(228,21.7%,9%)"
                },
                {
                    "name": "Denim",
                    "slug": "denim",
                    "color": "hsl(202,82%,50%)"
                },
                {
                    "name": "Denim Dark",
                    "slug": "denim-dark",
                    "color": "hsl(202,82%,22%)"
                },
                {
                    "name": "Concrete",
                    "slug": "concrete",
                    "color": "hsl(222,7%,53%)"
                },
                {
                    "name": "Stone",
                    "slug": "stone",
                    "color": "hsl(222,7%,75%)"
                },
                {
                    "name": "Gypsum",
                    "slug": "gypsum",
                    "color": "hsl(222,0%,90%)"
                }
            ],
            "gradients": [
                {
                    "name": "Circular from darkness to transparent, starting at top",
                    "slug": "circular-darkness",
                    "gradient": "radial-gradient(circle at top, var(--wp--preset--color--darkness) 10%, transparent 50%)"
                },
                {
                    "name": "Circular from darkness to transparent, starting at bottom",
                    "slug": "circular-darkness-reverse",
                    "gradient": "radial-gradient(circle at bottom, var(--wp--preset--color--darkness) 10%, transparent 50%)"
                },
                {
                    "name": "Linear from darkness to transparent, starting at top",
                    "slug": "linear-darkness",
                    "gradient": "linear-gradient(to bottom, var(--wp--preset--color--darkness) 20%, transparent 50%)"
                }
            ],
            "duotone": [],
            "defaultPalette" : false,
            "defaultGradients": false,
            "customGradient": false,
            "customDuotone": false
        },
        "typography": {
            "fluid": true,
            "fontFamilies": [
                {
                    "name": "Inter",
                    "slug": "inter",
                    "fontFamily": "Inter, Arial, sans-serif"
                },
                {
                    "name": "Roboto Slab",
                    "slug": "roboto-slab",
                    "fontFamily": "Roboto Slab, Georgia, serif"
                }
            ],
            "fontSizes": [
                {
                    "size": "0.875rem",
                    "fluid": {
                        "min": "0.875rem",
                        "max": "1rem"
                    },
                    "slug": "small",
                    "name": "Small"
                },
                {
                    "size": "1rem",
                    "fluid": {
                        "min": "1rem",
                        "max": "1.25rem"
                    },
                    "slug": "medium",
                    "name": "Medium"
                },
                {
                    "size": "1.375rem",
                    "fluid": {
                        "min": "1.375rem",
                        "max": "1.625rem"
                    },
                    "slug": "large",
                    "name": "Large"
                },
                {
                    "size": "2rem",
                    "fluid": {
                        "min": "2rem",
                        "max": "2.625rem"
                    },
                    "slug": "xlarge",
                    "name": "X-Large"
                },
                {
                    "size": "3rem",
                    "fluid": {
                        "min": "3rem",
                        "max": "4rem"
                    },
                    "slug": "xxlarge",
                    "name": "XX-Large"
                }
            ],
            "fontWeight": false,
            "fontStyle": false,
            "letterSpacing": false,
            "textDecoration": false,
            "textTransform": false,
            "dropCap": false
        },
        "custom": {
            "navbar": {
                "height": "5rem",
                "max-width": "80rem",
                "min-padding": "1rem"
            },
            "base": {
                "font-size": "clamp(1rem, calc(1rem + ((1vw - 0.3rem) * 0.5)), 1.25rem)",
                "gap": "calc(1.5 * var(--wp--custom--base--font-size))",
                "border-radius": "8px"
            },
            "content": {
                "max-width": "64rem",
                "min-padding": "2rem"
            },
            "teaserblock": {
                "image-width": "100px"
            }
        },
        "blocks": {
            "core/paragraph" : {
                "color": {
                    "custom": false,
                    "background": false
                }
            },
            "core/list" : {
                "color": {
                    "custom": false,
                    "background": false
                }
            },
            "core/heading": {
                "color": {
                    "custom": false,
                    "background": false
                }
            },
            "core/post-title": {
                "color": {
                    "custom": false,
                    "background": false,
                    "customGradient": false,
                    "gradients": [],
                    "defaultGradients": false
                }
            }
        }
    },
    "styles": {
        "elements": {
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--roboto-slab)",
                    "fontWeight": "600",
                    "lineHeight": "1.3",
                    "letterSpacing": "1.5px"
                },
                "color": {
                    "text": "var(--wp--preset--color--gypsum)"
                }
            },
            "link": {
                "typography": {
                    "fontWeight": "300",
                    "textDecoration": "none"
                },
                "color": {
                    "text": "var(--wp--preset--color--denim)"
                },
                "outline": "none",
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--white)"
                    }
                }
            },
            "caption": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--inter)",
                    "fontWeight": "300",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "1.5",
                    "letterSpacing": ".2px"
                },
                "color": {
                    "text": "var(--wp--preset--color--denim)"
                }
            },
            "button": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--inter)",
                    "fontWeight": "400",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "1.2",
                    "letterSpacing": ".4px",
                    "textTransform": "uppercase",
                    "textDecoration": "none"
                },
                "color": {
                    "text": "var(--wp--preset--color--denim)",
                    "background": "rgba(18,20,28,.5)"
                },
                "border": {
                    "color": "var(--wp--preset--color--denim)",
                    "radius": "5px",
                    "width": "2px",
                    "style": "solid"
                },
                "dimensions": {
                    "minHeight": "auto"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--white)",
                        "background": "var(--wp--preset--color--denim)"
                    }
                }
            }
        }
    },
    "customTemplates": [
        {
            "name": "single-publication",
            "title": "Single Publication",
            "postTypes": [ "publication" ]
        },
        {
            "name": "single-blog-post",
            "title": "Single Blog Post",
            "postTypes": [ "post" ]
        }
    ]
}
